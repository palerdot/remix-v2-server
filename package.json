{
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-p build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=esm ./server.ts --outdir=./",
    "dev": "remix dev -c \"npm run dev:server\" --manual",
    "dev:server": "tsx watch --clear-screen=false --ignore 'app/**' --ignore 'build/**' --ignore 'node_modules/**' --inspect ./server.ts",
    "start": "cross-env NODE_ENV=production node  server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/css-bundle": "*",
    "@remix-run/express": "*",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.8",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/source-map-support": "^0.5.6",
    "autoprefixer": "^10.4.14",
    "chokidar": "^3.5.3",
    "esbuild": "^0.19.4",
    "eslint": "^8.38.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.3.2",
    "tsx": "^3.14.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
